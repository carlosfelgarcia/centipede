{
  "scripts": [
    "*/*.py"
  ],
  "vars": {
    "temporaryDir": ""
  },
  "taskHolders": [
    {
      "task": "copy",
      "taskOptions": {
        "testOption": "{temporaryDir}",
        "testExpr": "(min 2 6)"
      },
      "targetTemplate": "{temporaryDir}/{baseName}",
      "matchTypes": [
        "exr"
      ],
      "matchVars": {
        "imageType": [
          "sequence"
        ]
      },
      "taskHolders": [
        {
          "task": "sequenceThumbnail",
          "targetTemplate": "{temporaryDir}/{name}.jpg",
          "matchTypes": [
            "testCrawler"
          ],
          "matchVars": {
            "imageType": [
              "sequence"
            ],
            "testVariable": true
          }
        }
      ]
    }
  ]
}
