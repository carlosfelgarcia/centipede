{
  "scripts": [
    "*/*.py"
  ],
  "vars": {
    "testCustomVar": "randomValue"
  },
  "taskHolders": [
    {
      "task": "copy",
      "targetTemplate": "{configPath}/{baseName}",
      "matchTypes": [
        "exr"
      ],
      "matchVars": {
        "imageType": [
          "sequence"
        ]
      },
      "taskHolders": [
      {
        "task": "sequenceThumbnail",
        "targetTemplate": "{configPath}/{name}.jpg",
        "matchTypes": [
          "testCrawler"
        ],
        "matchVars": {
          "imageType": [
            "sequence"
          ],
          "testVariable": true
        },
        "taskWrapper": "subprocess",
        "taskWrapperOptions": {
        "user": "$UMEDIA_VERSION_PUBLISHER_USER"
        }
      },
      {
        "task": "remove",
        "targetTemplate": "{filePath}"
      },
      {
        "task": "copy",
        "targetTemplate": "{configPath}/dummy.exr",
        "matchVars": {
          "dummyVariable": true
        }
      }
      ]
    }
  ]
}
