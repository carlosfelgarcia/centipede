{
  "scripts": [
    "*/*.py"
  ],
  "vars": {
    "testCustomVar": "randomValue"
  },
  "taskHolders": [
    {
      "task": "copy",
      "taskOptions": {
        "testOption": "{testCustomVar}",
        "testExpr": "(min 2 6)"
      },
      "targetTemplate": "(tmpdir)/{baseName}",
      "matchTypes": [
        "exr"
      ],
      "matchVars": {
        "imageType": [
          "sequence"
        ]
      },
      "taskHolders": [
        {
          "task": "sequenceThumbnail",
          "targetTemplate": "(tmpdir)/{name}.jpg",
          "matchTypes": [
            "testCrawler"
          ],
          "matchVars": {
            "imageType": [
              "sequence"
            ],
            "testVariable": true
          },
          "taskMetadata": {
            "taskWrapper.name": "subprocess",
            "taskWrapper.options": {
              "user": ""
            }
          }
        }
      ]
    }
  ]
}
