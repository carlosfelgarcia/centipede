{
  "scripts": [
    "*/*.py"
  ],
  "vars": {
    "prefix": "/data/job",
    "job": "999_r_d",
    "__uiHintSourceColumns": [
      "shot",
      "step",
      "variant",
      "type"
    ]
  },
  "taskHolders": [
    {
      "task": "createRenderVersion",
      "matchTypes": [
        "shotRender"
      ],
      "matchVars": {
      },
      "taskOptions": {
      },
      "taskWrapper": "subprocess",
      "taskWrapperOptions": {
        "user": "$UMEDIA_VERSION_PUBLISHER_USER"
      },
      "targetTemplate": "{prefix}/{job}/sequences/{seq}/{shot}/publish/{step}/{variant}/(newVersion <parentPath>)",
      "taskHolders": [
      {
          "task": "ffmpeg",
          "taskOptions": {
            "frameRate": 23.976,
            "sourceColorSpace": "bt709",
            "targetColorSpace": "smpte170m"
          },
          "taskWrapper": "subprocess",
          "taskWrapperOptions": {
            "user": "$UMEDIA_VERSION_PUBLISHER_USER"
          },
          "targetTemplate": "{dataPath}/{name}.mov",
          "matchTypes": [
            "exr"
          ],
          "matchVars": {
            "imageType": [
              "sequence"
            ]
          },
          "taskHolders": [
          {
              "task": "appendToVersion",
              "taskOptions": {
              },
              "taskWrapper": "subprocess",
              "taskWrapperOptions": {
                "user": "$UMEDIA_VERSION_PUBLISHER_USER"
              }
          }
          ]
        },
        {
          "task": "sgRenderPublish",
          "taskOptions": {
            "comment": "render publish"
          },
          "targetTemplate": "{filePath}"
        }
      ]
    }
  ]
}
