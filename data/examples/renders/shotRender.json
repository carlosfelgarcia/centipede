{
  "scripts": [
    "*/*.py"
  ],
  "vars": {
    "prefix": "/data/job",
    "job": "RND",
    "__uiHintSourceColumns": [
      "shot",
      "step",
      "pass",
      "type"
    ]
  },
  "taskHolders":
  [
  {
    "task": "createRenderVersion",
    "matchTypes": [
      "shotRender"
    ],
    "taskMetadata": {
      "taskWrapper.name": "subprocess",
      "taskWrapper.options": {
        "user": "$CENTIPEDE_VERSION_PUBLISHER_USER"
      }
    },
    "targetTemplate": "{prefix}/{job}/sequences/{seq}/{shot}/publish/{step}/{pass}/(newVersion <parentPath>)",
    "taskHolders":
    [
      {
        "task": "nukeTemplate",
        "taskOptions": {
          "topLeft": "{shot}",
          "topRight": "(yyyy)(mm)(dd)",
          "notes": "",
          "videocodec": "",
          "bottomLeft": "{pass}, {versionName}",
          "template": "{prefix}/{job}/config/centipede/mediaDelivery/templates/template.nk",
          "enableSlate": 0,
          "enableColorSpace": 0,
          "enableReformat": 0,
          "enableTimecode": 1
        },
        "targetTemplate": "{dataPath}/{name}.mov",
        "matchTypes": [
          "exr"
        ],
        "matchVars": {
          "imageType": [
            "sequence"
          ]
        },
        "taskMetadata": {
          "taskWrapper.name": "subprocess",
          "taskWrapper.options": {
            "user": "$CENTIPEDE_VERSION_PUBLISHER_USER"
          }
        },
        "taskHolders": [
          {
            "task": "appendToVersion",
            "taskMetadata": {
              "taskWrapper.name": "subprocess",
              "taskWrapper.options": {
                "user": "$CENTIPEDE_VERSION_PUBLISHER_USER"
              }
            },
          }
        ]
      },
      {
        "task": "sgPublish",
        "taskMetadata": {
          "taskWrapper.name": "upython2",
          "taskWrapper.options": {
            "publishedFileType": "Rendered Image",
            "comment": "render publish",
            "publishName": "{renderName}"
          }
        },
        "matchTypes": [
          "json"
        ]
      }
    ]
  }
]
}
