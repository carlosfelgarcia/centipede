{
  "scripts": [
    "*/*.py"
  ],
  "vars": {
    "prefix": "/data/job",
    "job": "RND",
    "__uiHintSourceColumns": [
      "shot",
      "step",
      "pass",
      "type"
    ]
  },
  "taskHolders":
  [
  {
    "task": "createRenderVersion",
    "matchTypes": [
      "shotRender"
    ],
    "taskWrapper": "subprocess",
    "taskWrapperOptions": {
      "user": "$CENTIPEDE_VERSION_PUBLISHER_USER"
    },
    "targetTemplate": "{prefix}/{job}/sequences/{seq}/{shot}/publish/{step}/{pass}/(newVersion <parentPath>)",
    "taskHolders":
    [
      {
        "task": "nukeTemplate",
        "taskOptions": {
          "topLeft": "{shot}",
          "topRight": "(yyyy)(mm)(dd)",
          "notes": "",
          "videocodec": "",
          "bottomLeft": "{pass}, {versionName}",
          "template": "{prefix}/{job}/config/centipede/mediaDelivery/templates/template.nk",
          "enableSlate": 0,
          "enableColorSpace": 0,
          "enableReformat": 0,
          "enableTimecode": 1
        },
        "targetTemplate": "{dataPath}/{name}.mov",
        "matchTypes": [
          "exr"
        ],
        "matchVars": {
          "imageType": [
            "sequence"
          ]
        },
        "taskWrapper": "subprocess",
        "taskWrapperOptions": {
          "user": "$CENTIPEDE_VERSION_PUBLISHER_USER"
        },
        "taskHolders": [
          {
            "task": "appendToVersion",
            "taskWrapper": "subprocess",
            "taskWrapperOptions": {
              "user": "$CENTIPEDE_VERSION_PUBLISHER_USER"
            }
          }
        ]
      },
      {
        "task": "sgPublish",
        "taskWrapper": "upython2",
        "taskOptions": {
          "publishedFileType": "Rendered Image",
          "comment": "render publish",
          "publishName": "{renderName}"
        },
        "matchTypes": [
          "json"
        ]
      }
    ]
  }
]
}
