{
  "scripts": [
    "*/*.py"
  ],
  "vars": {
    "prefix": "/data/job",
    "job": "RND",
    "__uiHintSourceColumns": [
      "shot",
      "step",
      "pass",
      "type"
    ]
  },
  "taskHolders":
  [
  {
    "task": "createRenderVersion",
    "matchTypes": [
      "shotRender"
    ],
    "taskWrapper": "subprocess",
    "taskWrapperOptions": {
      "user": "$UMEDIA_VERSION_PUBLISHER_USER"
    },
    "targetTemplate": "{prefix}/{job}/sequences/{seq}/{shot}/publish/{step}/{pass}/(newVersion <parentPath>)",
    "taskHolders":
    [
      {
        "task": "nukeTemplate",
        "taskOptions": {
          "topLeft": "{shot}",
          "topRight": "(yyyy)(mm)(dd)",
          "notes": "",
          "videocodec": "",
          "bottomLeft": "{pass}, {versionName}",
          "template": "/data/job/RND/config/ingestor/mediaDelivery/templates/template.nk",
          "addSlate": 0,
          "doColorSpace": 0,
          "doReformat": 0
        },
        "targetTemplate": "{dataPath}/{name}.mov",
        "matchTypes": [
          "exr"
        ],
        "matchVars": {
          "imageType": [
            "sequence"
          ]
        },
        "taskWrapper": "subprocess",
        "taskWrapperOptions": {
          "user": "$UMEDIA_VERSION_PUBLISHER_USER"
        },
        "taskHolders": [
          {
            "task": "appendToVersion",
            "taskWrapper": "subprocess",
            "taskWrapperOptions": {
              "user": "$UMEDIA_VERSION_PUBLISHER_USER"
            }
          }
        ]
      },
      {
        "task": "sequenceThumbnail",
        "targetTemplate": "{dataPath}/.thumbnail/{name}.jpg",
        "matchTypes": [
          "exr"
        ],
        "matchVars": {
          "imageType": [
            "sequence"
          ]
        },
        "taskWrapper": "subprocess",
        "taskWrapperOptions": {
          "user": "$UMEDIA_VERSION_PUBLISHER_USER"
        }
      },
      {
        "task": "sgImageSeqPublish",
        "taskOptions": {
          "publishedFileType": "Rendered Image",
          "movieFile": "{dataPath}/{name}.mov",
          "thumbnailFile": "{dataPath}/.thumbnail/{name}.jpg"
        },
        "matchTypes": [
          "exr"
        ],
        "matchVars": {
          "imageType": [
            "sequence"
          ]
        }
      }
    ]
  }
]
}
